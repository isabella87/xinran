	//相应的位置会定义 例如： data-div-name="新闻" 来找
	$(".ban_page_tool_index").click(function(e) {
		e.preventDefault();
		var href = $(this).attr('href');
		var name = href.substring(1);
		var target = $("div[data-div-name='"+name+"']");
		$(document).scrollTop(target.offset().top);
	});
});

